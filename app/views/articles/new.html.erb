<h1>articles#new</h1>
<p>Find me in app/views/articles/new.html.erb</p>

<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed %>
  <%= f.select :completed, [true, false]%>
  <%= f.submit %>
<% end %>
